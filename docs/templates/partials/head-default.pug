head
  block head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width')
    meta(name='description', content=site.description)

    title
      block title
        = site.title
    link(rel='author', href=owner.name)
    link(rel='stylesheet', href=relativePath + fingerprint['css/vendor/normalize.css'])
    link(rel='stylesheet', href=relativePath + fingerprint['css/vendor/rainbow.css'])
    link(rel='stylesheet', href=relativePath + fingerprint['css/vendor/flexboxgrid.css'])
    link(rel='stylesheet', href=relativePath + fingerprint['css/vendor/spectrum.css'])
    link(rel='stylesheet', href=relativePath + fingerprint['css/sdldocs.css'])

    script(type='text/javascript', src=relativePath + fingerprint['js/vendor/rainbow-custom.min.js'] )
    script(type='text/javascript', src=relativePath + fingerprint['js/vendor/typekit.js'] )
    script(type='text/javascript', src=relativePath + fingerprint['js/page-default.js'] )

